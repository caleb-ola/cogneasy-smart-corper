.log(user)} */}\r\n      <h1>Admin session sign in</h1>\r\n      <form onSubmit={handleSubmit}>\r\n        <input type=\"email\" value={client.email} onChange={handleEmail} />\r\n        <input\r\n          type=\"password\"\r\n          value={client.password}\r\n          onChange={handlePassword}\r\n        />\r\n        <button type=\"submit\" className=\"btn btn-primary mx-auto\">\r\n          LOGIN\r\n        </button>\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Admin;\r\n","C:\\Users\\C JAY\\Desktop\\Elab\\COGNEASY\\cogneasy-smart-corper\\src\\components\\dashboard.js",["81"],"import React, { useState, useEffect } from \"react\";\r\nimport XLSX from \"xlsx\";\r\nimport { db } from \"../firebase\";\r\n\r\nconst Dashboard = (props) => {\r\n  const [allUsers, setAllUsers] = useState([]);\r\n\r\n  const fetchData = () => {\r\n    db.collection(\"Corpers\")\r\n      .get()\r\n      .then((querySnapshot) => {\r\n        querySnapshot.forEach((doc) => {\r\n          // console.log(doc.id, \" => \", doc.data());\r\n          allUsers.push(doc.data());\r\n          setAllUsers([...allUsers]);\r\n          console.log(allUsers);\r\n        });\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetchData();\r\n  }, []);\r\n\r\n  const gatherdem = allUsers.map((item, index) => {\r\n    return (\r\n      <tr>\r\n        <td>{index + 1}</td>\r\n        <td>{item.fname}</td>\r\n        <td>{item.lname}</td>\r\n        <td>{item.email}</td>\r\n        <td>{item.num}</td>\r\n        <td>{item.exp}</td>\r\n      </tr>\r\n    );\r\n  });\r\n\r\n  const exportFile = () => {\r\n    let users = [[\"FirstName\", \"LastName\", \"Email\", \"Number\", \"Expectation\"]];\r\n    allUsers.forEach((user) => {\r\n      let userArray = [user.fname, user.lname, user.email, user.num, user.exp];\r\n      users.push(userArray);\r\n      setAllUsers((prev) => {\r\n        return [...prev];\r\n      });\r\n    });\r\n    const wb = XLSX.utils.book_new();\r\n    const wsAll = XLSX.utils.aoa_to_sheet(users);\r\n    XLSX.utils.book_append_sheet(wb, wsAll, \"All Users\");\r\n    XLSX.writeFile(wb, \"export-demo.xlsx\");\r\n  };\r\n  return (\r\n    <div>\r\n      <div className=\"admin-header\">\r\n        <h1>WELCOME ADMIN</h1>\r\n        <div>\r\n          <button className=\"btn btn-primary\" onClick={gatherdem}>\r\n            SHOW REGISTERED PARTICIPANTS\r\n          </button>\r\n          <button className=\"btn btn-success\" onClick={exportFile}>\r\n            EXPORT TO EXCEL\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <table style={{ width: \"100%\" }}>\r\n        <tr>\r\n          <th>S/N</th>\r\n          <th>First Name</th>\r\n          <th>Last Name</th>\r\n          <th>Email</th>\r\n          <th>Number</th>\r\n          <th>Expectation</th>\r\n        </tr>\r\n        {allUsers && gatherdem}\r\n      </table>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Dashboard;\r\n","C:\\Users\\C JAY\\Desktop\\Elab\\COGNEASY\\cogneasy-smart-corper\\src\\firebase.js",[],"C:\\Users\\C JAY\\Desktop\\Elab\\COGNEASY\\cogneasy-smart-corper\\src\\privateRoute.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":65,"column":15,"nodeType":"88","endLine":65,"endColumn":27},{"ruleId":"86","severity":1,"message":"87","line":68,"column":15,"nodeType":"88","endLine":68,"endColumn":27},{"ruleId":"86","severity":1,"message":"87","line":71,"column":15,"nodeType":"88","endLine":71,"endColumn":27},{"ruleId":"89","severity":1,"message":"90","line":43,"column":13,"nodeType":"91","messageId":"92","endLine":43,"endColumn":22},{"ruleId":"89","severity":1,"message":"93","line":44,"column":13,"nodeType":"91","messageId":"92","endLine":44,"endColumn":25},{"ruleId":"94","severity":1,"message":"95","line":23,"column":6,"nodeType":"96","endLine":23,"endColumn":8,"suggestions":"97"},"no-native-reassign",["98"],"no-negated-in-lhs",["99"],"jsx-a11y/anchor-is-valid